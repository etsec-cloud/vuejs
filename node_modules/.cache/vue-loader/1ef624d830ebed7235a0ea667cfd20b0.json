abled() {\n      if (this.list.lenght >= 1) {\n        console.log(\"true\");\n        return true;\n      }\n      console.log(\"false\");\n      return false;\n    }\n  },\n  methods: {\n    selectCompo(comp) {\n      this.selectedComponent = comp;\n    },\n    addPlayerToSelection(equipe) {\n      this.list.push(equipe);\n    },\n    suppPlayerToSelection(equipe) {\n      this.list.pop(equipe);\n    }\n  }\n};\n</script>\n\n<style></style>\n"]}]}